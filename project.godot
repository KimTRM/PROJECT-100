; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="PROJECT 100"
run/main_scene="res://Scenes/UI/LoginMenu/LoginMenu.tscn"
config/features=PackedStringArray("4.4", "C#", "Mobile")
boot_splash/bg_color=Color(0.0470588, 0.0470588, 0.0470588, 1)
boot_splash/image="res://Assets/ProjectFiles/KLTL.png"
config/icon="res://Assets/ProjectFiles/Title.png"
boot_splash/minimum_display_time=7

[autoload]

HttpManager="*res://Globals/HTTPManager.cs"
GameManager="*res://Globals/GameManager.cs"
SettingsMenu="*res://Scenes/UI/PauseMenu/SettingsMenu.tscn"
SceneTransition="*res://Scenes/Components/SceneTransition.tscn"
DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"
PauseMenu="*res://Scenes/UI/PauseMenu/PauseMenu.tscn"

[debug]

gdscript/warnings/untyped_declaration=1
gdscript/warnings/inferred_declaration=1

[dialogue_manager]

runtime/advanced/uses_dotnet=true
runtime/balloon_path="res://Dialouge/DialougeBalloon/Balloon.tscn"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
window/vsync/vsync_mode=2

[dotnet]

project/assembly_name="Project 100"

[editor]

naming/scene_name_casing=1

[editor_plugins]

enabled=PackedStringArray("res://addons/AsepriteWizard/plugin.cfg", "res://addons/dialogue_manager/plugin.cfg")

[file_customization]

folder_colors={
"res://Assets/": "yellow",
"res://Code/Conditionals/": "orange",
"res://Code/Loops/": "purple",
"res://Code/Symbols/": "blue",
"res://Code/Variables/": "red",
