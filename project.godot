"res://Scenes/": "teal",
"res://Scripts/": "orange",
"res://addons/": "blue",
"res://build/": "pink"
}

[gui]

theme/custom_font="res://Assets/ProjectFiles/Jaro-Regular.ttf"
theme/default_font_hinting=2
theme/default_font_subpixel_positioning=0
